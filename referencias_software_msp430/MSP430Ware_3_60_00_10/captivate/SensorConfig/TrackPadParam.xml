<?xml version="1.0" encoding="UTF-8"?>
<TuningParams>
    <Filter_Enable>
        <classification>sensor</classification>
        <type>boolean</type>
        <description><![CDATA[Enable or disable the wheel position filter.]]></description>
        <default>1</default>
        <min>0</min>
        <max>1</max>
        <cmd>0x90</cmd>
    </Filter_Enable>
    <Filter_Beta>
        <classification>sensor</classification>
        <type>int</type>
        <description><![CDATA[Specifies the wheel filter response characteristics.]]></description>
        <default>100</default>
        <min>0</min>
        <max>255</max>
        <cmd>0x91</cmd>
    </Filter_Beta>
    <Desired_Resolution>
        <classification>sensor</classification>
        <type>int</type>
        <description><![CDATA[Points of interest desired for the wheel. This determine the range of the wheel (1-n)]]></description>
        <default>1000</default>
        <min>2</min>
        <max>65535</max>
        <cmd>0x92</cmd>
    </Desired_Resolution>
</TuningParams>

